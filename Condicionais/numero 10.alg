algoritmo "semnome"
// Função :
// Autor :
// Data : 18/05/2023
// Seção de Declarações 
var
num: real
resultado: logico

inicio

//entrada

escreval( "digite o numero: ")
leia (num)

//processamento

se (num >= 10) entao
 escreval ("sim, e maior")
 
senao
 escreval( "sim, e menor")
 
fimse

fimalgoritmo
